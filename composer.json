{
    "name":"harikt/conduit-skelton",
    "description": "Conduit skelton with router middleware, authentication middleware, negotiation middleware.",
    "keywords": [
        "skelton",
        "middleware",
        "router",
        "authentication"
    ],
    "homepage": "https://github.com/harikt/conduit-skelton",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Hari KT",
            "homepage": "http://harikt.com"
        }
    ],
    "require": {
        "phly/conduit": "0.10.*",
        "aura/router": "~2.2",
        "aura/dispatcher": "~2.0",
        "aura/di": "~2.0",
        "vlucas/phpdotenv": "~1.1",
        "twig/twig": "~1.0",
        "aura/auth": "2.0.*@dev",
        "aura/session": "2.0.*@dev",
        "foa/auth-session-bundle": "0.1.*@dev",
        "aura/filter": "2.0.*@dev",
        "aura/accept": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "Skelton\\_Config\\": "config/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
